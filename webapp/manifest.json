{
	"_version": "1.12.0",
	"sap.app": {
		"id": "moizaled.SAPUI5",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"dataSources": {

			"remoteData": {
				"uri": "https://services.odata.org/V3/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {

			"viewName": "moizaled.SAPUI5.view.App",
			"type": "XML",
			"async": "true",
			"id": "app"

		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "moizaled.SAPUI5.i18n.i18n"
				}
			},
			"invoice": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"dataSource": "remoteData"
			}

		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},

		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "moizaled.SAPUI5.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": true
			},

			"routes": [{
					"pattern": "",
					"name": "overview",
					"target": "overview"
				},

				{
					"pattern": "detail/{invoicePath}",
					"name": "detail",
					"target": "detail"

				}
			],

			"targets": {
				"overview": {
					"viewId": "overview",
					"viewName": "Overview"

				},

				"detail": {

					"viewId": "detail",
					"viewName": "Detail"
				}
			}

		}

	}
}